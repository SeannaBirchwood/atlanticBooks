<article class="user">
  <header>
    <%= link_to users_profile_path do %>
      <%= image_tag user.avatar, alt: user.username %>
      <h4><%= user.username %></h4>
    <% end %>
  </header>
  <p class="description">
    <%= user.location %>
  </p>

  <aside class="price">
    <%= user.wishlist %>
  </aside>
</article>
